<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for UC Number -->
        <record id="internal_reference_sequence" model="ir.sequence">
            <field name="name">UC Number Sequence</field>
            <field name="code">product.uc.number</field>
            <field name="implementation">standard</field>
            <field name="prefix">UC</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next_actual">1</field>
        </record>

        <!-- Sequence for Lot Number -->
        <record id="garment_receipt_lot_sequence" model="ir.sequence">
            <field name="name">Garment Receipt Lot Sequence</field>
            <field name="code">stock.move.line</field>
            <field name="implementation">standard</field>
            <field name="prefix">GR/LOT/</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next_actual">1</field>
        </record>

        <!-- Sequence for BOM Number -->
        <record id="bom_lot_sequence" model="ir.sequence">
            <field name="name">BOM Sequence</field>
            <field name="code">bom.code</field>
            <field name="implementation">standard</field>
            <field name="prefix">BOM/</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next_actual">1</field>
        </record>


        <!-- Sequence for UC Reference Number -->
        <record id="uc_reference_sequence" model="ir.sequence">
            <field name="name">UC Reference</field>
            <field name="code">product.uc.number</field>
            <field name="implementation">standard</field>
            <field name="prefix">UC/</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next_actual">1</field>
        </record>


        <!-- Sequence for Parent Manufacture Order -->
        <record id="parent_manufacture_order_sequence" model="ir.sequence">
            <field name="name">Parent Manufacture Order</field>
            <field name="code">parent.manufacture.order</field>
            <field name="implementation">standard</field>
            <field name="prefix">PMO/</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next_actual">1</field>
        </record>


        <!-- Sequence for Quality Check -->
        <record id="quality_check_sequence" model="ir.sequence">
            <field name="name">Quality Check</field>
            <field name="code">quality.check.number</field>
            <field name="implementation">standard</field>
            <field name="prefix">QC/</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next_actual">1</field>
        </record>


        <!-- Sequence for Purchase Requisition -->
        <record id="purchase_requisition_sequence" model="ir.sequence">
            <field name="name">Purchase Requisition</field>
            <field name="code">purchase.requisition</field>
            <field name="implementation">standard</field>
            <field name="prefix">PR/</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next_actual">1</field>
        </record>


    </data>
</odoo>